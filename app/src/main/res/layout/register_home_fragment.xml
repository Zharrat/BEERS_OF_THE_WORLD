<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo5"
    tools:context=".ui.fragments.RegisterFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tvusername"
            android:layout_width="207dp"
            android:layout_height="27dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="293dp"
            android:layout_marginEnd="147dp"
            android:layout_marginBottom="411dp"
            android:text="@string/UserNameTextView"
            android:textAlignment="viewStart"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etpassword"
            android:layout_width="247dp"
            android:layout_height="38dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="449dp"
            android:layout_marginEnd="108dp"
            android:layout_marginBottom="244dp"
            android:ems="10"
            android:hint="Password"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvpassword"
            android:layout_width="251dp"
            android:layout_height="32dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="409dp"
            android:layout_marginEnd="103dp"
            android:layout_marginBottom="290dp"
            android:text="Password:"
            android:textAlignment="viewStart"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etlastname"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="57dp"
            android:layout_marginTop="224dp"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="467dp"
            android:ems="10"
            android:hint="Enter Last Name"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvfirstname"
            android:layout_width="181dp"
            android:layout_height="28dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="65dp"
            android:layout_marginEnd="173dp"
            android:layout_marginBottom="638dp"
            android:text="First Name:"
            android:textAlignment="viewStart"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etusername"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="57dp"
            android:layout_marginTop="331dp"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="360dp"
            android:ems="10"
            android:hint="Enter User Name"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvlastname"
            android:layout_width="146dp"
            android:layout_height="34dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="181dp"
            android:layout_marginEnd="208dp"
            android:layout_marginBottom="516dp"
            android:text="Last Name"
            android:textAlignment="viewStart"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etfirstname"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Enter First Name"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textSize="14sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="57dp"
            tools:layout_editor_absoluteY="109dp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="218dp"
            android:layout_height="64dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="544dp"
            android:layout_marginEnd="92dp"
            android:layout_marginBottom="123dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/signbutton"
                android:layout_width="218dp"
                android:layout_height="64dp"
                android:background="@drawable/fondo8"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/tvregister"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="REGISTER"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/signbutton"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/signbutton"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="16dp" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>

